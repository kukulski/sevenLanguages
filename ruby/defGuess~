#!/usr/bin/ruby

def doguess
begin
  guess = gets
  guessInt = guess.to_i
  puts "too high" if guessInt > ournumber
  puts "too low" if guessInt < ournumber
  guessInt == ournumber
end 

puts "I'm thinking of a number between one and ten (or maybe ten)"
ournumber = rand(10) + 1

while doguess


puts "that's it. I WAS thinking #{ournumber}!"
